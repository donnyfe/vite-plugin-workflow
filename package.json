{
	"name": "@idonnyfe/vite-plugin-workflow",
	"version": "1.0.1-alpha.11",
	"description": "一个用于优化工作流的 vite 插件包，提供多种实用vite插件",
	"author": "donnyfe",
	"homepage": "https://github.com/donnyfe/vite-plugin-workflow/README.md",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/donnyfe/vite-plugin-workflow.git"
	},
	"keywords": [
		"vite-plugin-workflow",
		"workflow",
		"vite-plugin",
		"deploy server",
		"deploy npm"
	],
	"files": [
		"dist"
	],
	"license": "MIT",
	"type": "module",
	"main": "./dist/index.cjs.js",
	"module": "./dist/index.es.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.es.js",
			"require": "./dist/index.cjs.js",
			"types": "./dist/index.d.ts"
		}
	},
	"publishConfig": {
		"registry": "https://registry.npmjs.org",
		"tag": "alpha",
		"access": "public",
		"directory": "dist",
		"scope": "@idonnyfe"
	},
	"scripts": {
		"clean": "rm -rf dist",
		"build": "npm run clean && vite build --config vite.config.js"
	},
	"devDependencies": {
		"@types/node": "^22.9.0",
		"vite": "^5.4.11",
		"vite-plugin-dts": "^4.3.0"
	},
	"dependencies": {
		"node-ssh": "^13.2.0"
	}
}
